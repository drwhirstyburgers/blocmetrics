<h1><%= @registered_application.app_name %></h1>
<div class="row">
  <div class="col-md-8">
    <%= form_for @registered_application do |f| %>
      <div class="form-group">
        <%= f.label :registration %>
        <%= f.text_field :registration, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :app_name %>
        <%= f.text_field :app_name, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>
